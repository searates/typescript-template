{
  "name": "react-typescript-template",
  "version": "1.0.0",
  "description": "webpack template for react with typescript stack",
  "scripts": {
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.tsx\"",
    "prettier:fix": "npm run prettier:cli -- --write",
    "dev:serve": "cross-env NODE_ENV=development SERVE=development webpack-dev-server --config ./webpack",
    "dev": "cross-env NODE_ENV=development webpack --watch --config ./webpack",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack",
    "remove-redux": "rm -rf ./node_modules && rm -rf ./package-lock.json && rm -rf ./src/redux && npm uninstall redux react-redux redux-thunk redux-devtools-extension redux-immutable-state-invariant && npm install"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.0",
    "compression-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prettier-webpack-plugin": "^1.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable-state-invariant": "^2.1.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^1.4.4",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
